{
  "expo": {
    "name": "Veldt",
    "slug": "veldt",
    "scheme": "veldt",
    "description": "Veldt - Service Booking everywhere in Namibia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a237e",
      "enableFullScreenImage_legacy": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gewersdeon.veldt",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a237e"
      },
      "package": "com.gewersdeon.veldt"
    },
    "web": {
      "favicon": "./assets/apple-touch-icon.png"
    },
    "owner": "gewersdeon",
    "runtimeVersion": "1.0.0",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.exe.dev/66e649e4-be36-43c1-accd-a2713b5f4348"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "d323d87d-0def-4cc1-a08f-e2801636f0d1"
      }
    },
    "plugins": [
      "expo-font",
      "@react-native-community/datetimepicker"
    ]
  }
}
